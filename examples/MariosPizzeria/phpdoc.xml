<?xml version="1.0" encoding="UTF-8" ?>
<phpdocumentor configVersion="3.0" title="Simple Template Example">
  <paths>
    <output>docs</output>
    <cache>tmp/docs-cache</cache>
  </paths>
  <version number="3.0">
    <api format="php">
      <source dsn=".">
      <path>src</path>
      </source>
      <default-package-name>Marios</default-package-name>
      <include-source>true</include-source>
    </api>
  </version>
  <template name="clean">
  </template>
</phpdocumentor>