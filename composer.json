{
    "name": "kranemora/phpdocumentor-simple-template",
    "description": "Simple template for phpDocumentor",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Fernando Pita",
            "email": "kranemora@gmail.com"
        }
